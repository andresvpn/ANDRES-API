<script>
// Simulando desplazamiento infinito
const panels = document.querySelectorAll('.panel');
const container = document.querySelector('.container');

container.addEventListener('scroll', () => {
    const lastPanel = panels[panels.length - 1];
    const lastPanelRect = lastPanel.getBoundingClientRect();
    const containerRect = container.getBoundingClientRect();

    if (lastPanelRect.bottom <= containerRect.bottom) {
        const clonedPanel = lastPanel.cloneNode(true);
        container.appendChild(clonedPanel);
    }
});
</script>
